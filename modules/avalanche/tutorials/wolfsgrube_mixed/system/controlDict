FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application         faTwoLayerAvalancheFoam;

startFrom           startTime;

startTime           0;

stopAt              endTime;

endTime             90;

deltaT              0.002;

writeControl        adjustableRunTime;

writeInterval       2;

purgeWrite          0;

writeFormat         ascii;

writePrecision      6;

writeCompression    off;

timeFormat          general;

timePrecision       6;

runTimeModifiable   yes;

adjustTimeStep      yes;

initDeltaT          no;

maxCo               0.7;

functions
{
    #include "autoAreaToVolumeMapping"
}

